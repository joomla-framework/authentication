{
    "name": "joomla/authentication",
    "type": "joomla-package",
    "description": "Joomla Authentication Package",
    "keywords": ["joomla", "framework", "authentication"],
    "homepage": "https://github.com/joomla-framework/authentication",
    "license": "LGPL-2.1+",
    "require": {
        "php": ">=5.3.10"
    },
    "require-dev": {
        "joomla/input": "~1.0",
        "joomla/test": "~1.0",
        "ircmaxell/password-compat": "~1.0",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "suggest": {
        "ircmaxell/password-compat": "To use the LocalStrategy object in PHP <5.5, install the ircmaxell/password-compat package",
        "joomla/input": "To use the LocalStrategy object, install the joomla/input package"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Authentication\\": "src/",
            "Joomla\\Authentication\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
